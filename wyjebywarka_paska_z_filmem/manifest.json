{
  "name": "Wyjebywarka górnego paska z joemonster.org",
  "version": "1.0.1",
  "permissions": ["tabs", "*://joemonster.org/*"],

"externally_connectable": {
    "ids": [
      "*"
    ]},
  "description": "Wyjebuje górny pasek z joemonster.org",
  "background": {"page": "background.html"},
  "manifest_version": 2,
 "background": {
    "scripts": ["jquery-2.0.2.min.js","background.js"]
 },       
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "content.js" ],
    "matches": ["http://joemonster.org/*"]
  }]
}